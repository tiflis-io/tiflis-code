{
  "name": "@tiflis-io/tiflis-code-workstation",
  "version": "0.3.8",
  "description": "Workstation server for tiflis-code - manages agent sessions and terminal access",
  "author": "Roman Barinov <rbarinov@gmail.com>",
  "license": "FSL-1.1-NC",
  "repository": {
    "type": "git",
    "url": "https://github.com/tiflis-io/tiflis-code.git",
    "directory": "packages/workstation"
  },
  "homepage": "https://github.com/tiflis-io/tiflis-code#readme",
  "bugs": {
    "url": "https://github.com/tiflis-io/tiflis-code/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "type": "module",
  "main": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "types": "./dist/main.d.ts"
    }
  },
  "bin": {
    "tiflis-code-workstation": "./dist/main.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx watch src/main.ts",
    "build": "tsup",
    "start": "node dist/main.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@langchain/core": "^0.3.26",
    "@langchain/langgraph": "^0.2.42",
    "@langchain/openai": "^0.3.17",
    "better-sqlite3": "^11.7.0",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.38.3",
    "fastify": "^5.2.1",
    "formdata-node": "^6.0.3",
    "nanoid": "^5.0.9",
    "node-pty": "^1.0.0",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "ws": "^8.18.0",
    "zod": "^3.24.1",
    "qrcode": "^1.5.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^22.10.2",
    "@types/ws": "^8.5.13",
    "@types/qrcode": "^1.5.5",
    "drizzle-kit": "^0.30.1",
    "eslint": "^9.17.0",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.1",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=24.0.0"
  }
}
