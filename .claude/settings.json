{
  "permissions": {
    "deny": [
      "Read(**/.env)",
      "Read(**/.env.local)",
      "Read(**/.env.*.local)",
      "Read(**/.env.development)",
      "Read(**/.env.test)",
      "Read(**/.env.production)",
      "Read(**/*.pem)",
      "Read(**/*.key)",
      "Read(**/*.p12)",
      "Read(**/*.pfx)",
      "Read(**/*.crt)",
      "Read(**/*.cer)",
      "Read(**/*.jks)",
      "Read(**/.netrc)",
      "Read(**/.npmrc.local)",
      "Read(**/secrets/**)",
      "Read(**/.secrets/**)",
      "Read(**/signing.properties)",
      "Read(**/local.properties)",
      "Read(**/*.mobileprovision)",
      "Read(**/*.provisionprofile)",
      "Read(~/.ssh/**)",
      "Read(~/.aws/**)",
      "Read(~/.netrc)",
      "Read(~/.npmrc)"
    ]
  }
}
